	public static void main(String[] args) 
	{
		//ArrayList
		ArrayList<Integer> arrayList = new ArrayList<Integer>();
		
		arrayList.add(101);
		arrayList.add(102);
		arrayList.add(103);
		arrayList.add(104);
		arrayList.add(105);
		
		
		//Array
		Integer array[] = new Integer(arrayList.size());
		
		
		
		
	}

}
