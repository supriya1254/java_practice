package Inheritance;

class DemoOut
{
  
	private int i;
	
	
	
	class DemoInn
	{
		void display()
		{
			System.out.println("print i : " + i);
		}
	}


}

class ChildDemo extends DemoOut
{
	DemoInn inn  = new DemoInn();
	
}

public class SuperClass 
{
	public static void main(String[] args) 
	{
		DemoOut out = new DemoOut();
		
		
		
		
	}

}


